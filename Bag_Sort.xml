<Ui xmlns="http://www.blizzard.com/wow/ui/"   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   xsi:schemaLocation="http://www.blizzard.com/wow/ui/"> 
   <Script file="Bag_Sort.lua"/> 

   <!-- Frame to handle all core events --> 
   <Frame name="Bag_Sort_Core"> 
      <Scripts> 
         <OnLoad>
	   BS_OnLoad()
         </OnLoad>
         <OnEvent>
           BS_OnEvent()
         </OnEvent>
         <OnUpdate>
           BS_OnUpdate()
         </OnUpdate>
      </Scripts> 
   </Frame> 

   <GameTooltip name="BS_toolTip" inherits="GameTooltipTemplate"/>

</Ui>